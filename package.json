{
    "name": "sparray",
    "version": "1.0.8",
    "description": "Super Array",
    "main": "dist/sparray.js",
    "types": "dist/sparray.d.ts",
    "scripts": {
        "prepare": "npm run build",
        "clean": "rm -rf dist dist-browser docs",
        "build": "tsc && tsc -p tsconfig-browser.json",
        "watch": "tsc -w && tsc -w -p tsconfig-browser.json",
        "lint": "tslint -p tsconfig.json",
        "doc": "jsdoc2md dist/sparray.js",
        "cover": "nyc --extension=.ts --reporter=lcov --reporter=html --reporter=text mocha --timeout 15000 --exit test/*js",
        "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js -v",
        "docs": "typedoc --out docs --includeVersion",
        "test": "mocha --timeout 15000 --bail --exit test/*-test.js test/security/*.js",
        "deploy": "npm run build && npm test && npm version patch && git push && git push --tag"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/pcandido/sparray.git"
    },
    "keywords": [
        "list",
        "array",
        "collection"
    ],
    "author": "Paulo CÃ¢ndido <pcandido.m@gmail.com>",
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/pcandido/sparray/issues"
    },
    "homepage": "https://github.com/pcandido/sparray#readme",
    "devDependencies": {
        "@types/node": "^13.9.1",
        "mocha": "^7.0.1",
        "tslint": "^6.1.0",
        "typedoc": "^0.17.3",
        "typescript": "^3.8.3"
    },
    "files": [
        "dist",
        "dist-browser"
    ]
}
